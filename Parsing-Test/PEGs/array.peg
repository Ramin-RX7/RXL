#(?P<Indent>\s*)array \s*(?P<VarName>\w+)\s*\[\s*(?P<Length>\w+)?\s*:?\s*(?P<Type>\w+)?\s*\]\s*=\s*{(?P<Content>.*)}\s*
grammar ARRAY
    array  <-   indent:s "array " s varname:var s "[" options "]" s "=" s "{" values:[^}]* "}" s

    options <- (s len:w? s (":" s type:var)?)? s
    s    <-   " "*
    var  <-   [a-zA-Z_]+ [a-zA-Z0-9_]*
    w    <-   [a-zA-Z0-9_]*