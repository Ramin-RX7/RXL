#(?P<Indent>\s*)array \s*(?P<VarName>\w+)\s*\[\s*((?P<Type>.+)?\s*:(\s*(?P<Length>.+))?\s*)?\]\s*=\s*<(?P<Content>.*)>\s*

grammar ARRAY
    array  <-   !w "array " s "[" options "]" s "<" values:[^>]* ">"

    options <- s (type_:.* s ":" s max_length:.* s)
    s    <-   " "*
    w    <-   [a-zA-Z0-9_]